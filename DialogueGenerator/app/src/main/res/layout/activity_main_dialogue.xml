<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_dialogue"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
android:background="@color/algae"
    tools:context="com.example.sonali.dialoguegenerator.MainActivity_Dialogue">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:foregroundGravity="center"
        android:layout_centerInParent="true"
        android:src="@drawable/thumbsup"
        android:id="@+id/ThumbsUp"


        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textColor="@color/colorWhite"
        android:text="Click On The Image Below"

android:textStyle="bold"
        android:textSize="20dp"
        android:layout_above="@+id/ThumbsUp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="35dp"
        android:id="@+id/clickHereText" />
</RelativeLayout>
